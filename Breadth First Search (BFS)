#include <bits/stdc++.h>
using namespace std;
void bfs(int start, vector<vector<int>>& adj) {
    vector<bool> visited(adj.size(), false);
    queue<int> q;
    q.push(start);
    visited[start] = true;
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";
        for (int neighbor : adj[node])
            if (!visited[neighbor]) {
                visited[neighbor] = true;
                q.push(neighbor);
            }
    }
}
int main() {
    int n = 5;
    vector<vector<int>> adj = {{1, 2}, {0, 3}, {0, 3, 4}, {1, 2}, {2}};
    bfs(0, adj);
}
